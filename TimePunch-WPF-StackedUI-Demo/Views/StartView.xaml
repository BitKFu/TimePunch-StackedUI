<Page x:Class="TimePunch.StackedUI.Demo.Views.StartView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TimePunch.StackedUI.Demo.Views"
      xmlns:extensions1="clr-namespace:TimePunch.StackedUI.Extensions;assembly=TpStackedUI"
      mc:Ignorable="d"
      extensions1:StackedFrameExtension.FrameKey="StartView"
      DataContext="{Binding Source={StaticResource ViewModelLocator}, Path=StartViewModel}"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StartView">

    <Border BorderBrush="Chartreuse" BorderThickness="1">
        <Grid>
            <StackPanel Orientation="Vertical">
                <Button Content="Demo 1" Margin="12" Command="{Binding Demo1Command}"></Button>
            </StackPanel>
        </Grid>
    </Border>
</Page>
